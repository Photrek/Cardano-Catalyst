Cell[BoxData[
 RowBox[{"Manipulate", "[", 
  RowBox[{
   RowBox[{"LogLogPlot", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Evaluate", "[", 
      RowBox[{"{", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"Tooltip", "[", 
           RowBox[{
            RowBox[{"DelegateWeight", "[", 
             RowBox[{"#", ",", "10", ",", "wallets", ",", "powerDelegate"}], 
             "]"}], ",", "#"}], "]"}], "&"}], "/@", 
         RowBox[{"{", 
          RowBox[{
           SuperscriptBox["10", "4"], ",", 
           SuperscriptBox["10", "5"], ",", 
           SuperscriptBox["10", "6"], ",", 
           SuperscriptBox["10", "7"], ",", 
           SuperscriptBox["10", "8"], ",", 
           SuperscriptBox["10", "9"], ",", 
           SuperscriptBox["10", "10"]}], "}"}]}], ",", "\[IndentingNewLine]", 
        
        RowBox[{
         RowBox[{
