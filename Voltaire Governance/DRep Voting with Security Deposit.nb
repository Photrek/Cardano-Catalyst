 RowBox[{"\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"Votes", "[", 
      RowBox[{
      "delegate_", ",", "deposit_", ",", "minDelegate_", ",", "minDeposit_", 
       ",", "wallets_", ",", 
       RowBox[{"powerDelegate_", ":", "0.5"}], ",", 
       RowBox[{"powerDeposit_", ":", "2"}]}], "]"}], ":=", 
     RowBox[{"Min", "[", 
      RowBox[{
       RowBox[{"DelegateWeight", "[", 
        RowBox[{
        "delegate", ",", "minDelegate", ",", "wallets", ",", 
         "powerDelegate"}], "]"}], ",", 
       RowBox[{"DepositWeight", "[", 
        RowBox[{
        "deposit", ",", "minDeposit", ",", "wallets", ",", "powerDeposit"}], 
        "]"}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"DelegateWeight", "[", 
      RowBox[{"delegate_", ",", 
       RowBox[{"minDelegate_", ":", "10"}], ",", "wallets_", ",", 
